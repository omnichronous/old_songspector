<div class="panel panel-default">
  <div class="panel-heading">Video</div>
  <div class="panel-body">
    <div class="row">
      <div class="col-md-12 col-lg-12">
        <% if (typeof(errors["videosStatus"]) !== 'undefined') { %>
          <%= errors["videosStatus"] %>
        <% } %>
        <% if (typeof(videos) !== 'undefined') { %>
        <div class="well well-sm">
          <div class="embed-responsive embed-responsive-4by3">
            <iframe class="embed-responsive-item" src="https://www.youtube.com/embed/<%= videos.items[0].id.videoId %>"></iframe>
          </div>
        </div>
        <% } %>
      </div>
      <% if (typeof(videos) !== 'undefined') { %>
        <% videos.items.forEach(function(video, i) { %>
        <% if (i>0) { %>
        <div class="col-md-6 col-lg-6">
          <div class="panel panel-default">
            <!--<div class="panel-heading"><h5><%= video.type %></h5></div>-->
            <div class="panel-body">
               <a href="https://www.youtube.com/watch?v=<%= video.id.videoId %>" title="<%= video.snippet.title %>">
                <img class="img-responsive" src="<%= video.snippet.thumbnails.medium.url %>" />
              </a>
            </div>
          </div>
        </div>
        <% } %>
        <% }); %>
      <% } %>
    </div>
  </div>
</div>